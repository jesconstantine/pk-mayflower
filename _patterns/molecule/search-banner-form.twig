{% set form = form.content %}
<form class="ma__search-banner__form" action="{{form.action}}">
  <div class="ma__search-banner__input">
    {% set inputText = form.inputText %}
    {% include "@atoms/forms/input-text.twig" %}
  </div>
  <button class="ma__search-banner__button" type="submit">
    <span>{{form.buttonText}}</span>
    {% include "@atoms/icons/svg-search.twig" %}
  </button>
</form>
